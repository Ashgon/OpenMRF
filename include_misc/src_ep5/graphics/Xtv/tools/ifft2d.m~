function out=ifft2d(in,zf);

dim = size(in);


tmp1 = ifft1d(in,)



if nargin == 2
    a = ones(1,ndims(in));
    a(1,end-1:end) = zf;
    tmp1 = matresize(in,round(size(in).*a./2)*2);
else
    
    
    tmp1 = in;
end

